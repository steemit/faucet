{
  "name": "faucet",
  "type": "module",
  "version": "1.0.1",
  "description": "Steemit Register Program",
  "main": "./bin/www",
  "scripts": {
    "start": "./bin/www",
    "start-dev": "nodemon --ignore src ./bin/www | pino-pretty",
    "jest": "env $(tr \"\\\\n\" \" \" < .env.example) NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "test": "yarn run lint && yarn run jest",
    "build": "webpack --config webpack/webpack.prod.config.js",
    "build-dev": "webpack --progress --config webpack/webpack.config.js",
    "precommit": "lint-staged",
    "db:migrate": "env $(tr \"\\\\n\" \" \" < .env) sequelize-cli db:migrate --config db/config/config.json --migrations-path db/migrations --seeders-path db/seeders --models-path db/models"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steemit/faucet.git"
  },
  "keywords": [
    "steem"
  ],
  "author": "ety001",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steemit/faucet/issues"
  },
  "homepage": "https://github.com/steemit/faucet#readme",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@eslint/js": "^9.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.3.0",
    "webpack-hot-middleware": "^2.26.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@sendgrid/mail": "^8.1.3",
    "@steemit/steem-js": "^0.7.11",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "google-libphonenumber": "^3.2.38",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "maxmind": "^4.3.21",
    "moment": "^2.30.1",
    "mysql2": "^3.11.0",
    "pino": "^9.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sequelize": "^6.37.3",
    "tronweb": "^5.3.2",
    "twilio": "^5.2.2",
    "validator": "^13.12.0"
  }
}
