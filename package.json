    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.3",
    "react-refresh": "^0.14.2",
    "sequelize-cli": "^6.6.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
